{
	"extends": [
		"tslint-config-airbnb",
		"tslint-eslint-rules",
		"tslint-config-prettier"
	],
	"rules": {
		"no-implicit-dependencies": [true, "dev"],
		"ordered-imports": false,
		"no-console": false,
		"import-name": false,
		"strict-boolean-expressions": false,
		"variable-name": false,
		"jsx-no-lambda": false,
		"function-name": false,
		"no-unused-variable": true,
		"prettier": true
	},
	"rulesDirectory": ["tslint-plugin-prettier"]
}
